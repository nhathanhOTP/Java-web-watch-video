	<script>
		// When the user scrolls down 50px from the top of the document, resize the header's font size
		window.onscroll = function() {
			scrollFunction()
		};

		function scrollFunction() {
			if (document.body.scrollTop > 80
					|| document.documentElement.scrollTop > 80) {
				document.getElementById("header").style.fontSize = "18px";
				document.getElementsByClassName("rowd")[0].style.top = "0";
			} else {
				document.getElementById("header").style.fontSize = "26px";
				document.getElementsByClassName("rowd")[0].style.top = "30px";
			}
		}
